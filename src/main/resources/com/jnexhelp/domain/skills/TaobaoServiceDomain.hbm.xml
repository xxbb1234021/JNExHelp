<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Hibernate映射文件的根元素 -->
<hibernate-mapping package="com.jnexhelp.domain.skills">
	<class	name="TaobaoServiceDomain"	table="TaobaoService_table">
		<!-- 使用只读缓存 -->
		<cache usage="read-only"/>
		<!-- 映射标识属性 -->

		<id	name="id" type="integer" column="id">
			<!-- 指定使用identity主键生成策略 -->
			<generator class="identity"/>
		</id>

		<!-- 映射普通属性 -->
		<property name="userId" column="userId"
			type="string"/>
		<property name="onlineMarketing" column="onlineMarketing"
			type="boolean"/>
		<property name="photoShop" column="photoShop"
			type="boolean"/>
		<property name="taobaoRanking" column="taobaoRanking"
			type="boolean"/>


	</class>
</hibernate-mapping>