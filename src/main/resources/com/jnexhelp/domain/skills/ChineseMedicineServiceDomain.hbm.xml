<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Hibernate映射文件的根元素 -->
<hibernate-mapping package="com.jnexhelp.domain.skills">
	<class	name="ChineseMedicineServiceDomain"	table="ChineseMedicineService_table">
		<!-- 使用只读缓存 -->
		<cache usage="read-only"/>
		<!-- 映射标识属性 -->

		<id	name="id" type="integer" column="id">
			<!-- 指定使用identity主键生成策略 -->
			<generator class="identity"/>
		</id>

		<!-- 映射普通属性 -->
		<property name="userId" column="userId"
			type="string"/>
		<property name="palmprintDiagnosis" column="palmprintDiagnosis"
			type="boolean"/>
		<property name="guaSha" column="guaSha"
			type="boolean"/>
		<property name="cuppingTherapy" column="cuppingTherapy"
			type="boolean"/>
		<property name="chineseMassage" column="chineseMassage"
			type="boolean"/>
			
	</class>
</hibernate-mapping>